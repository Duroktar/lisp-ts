{
  "name": "lisp-ts",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "bin": {
    "tscheme": "scripts/repl.sh"
  },
  "scripts": {
    "bundle": "webpack",
    "bundle:dev": "webpack --watch",
    "build:watch": "tsc --watch",
    "build": "tsc",
    "repl:dev": "nodemon --config nodemon.json src/repl.ts",
    "repl:watch": "nodemon --config nodemon.json dist/repl.js",
    "repl": "ts-node src/repl.ts",
    "test:dev": "nodemon --config tests/nodemon.json src/start.ts",
    "test:rl": "nodemon src/test-rl.ts",
    "test:read": "nodemon dist/core/read.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "start": "node dist/start.js",
    "start:dev": "nodemon --config tests/nodemon.json dist/start.js"
  },
  "devDependencies": {
    "@trabpukcip/inject": "^0.0.12",
    "@types/chai": "^4.3.0",
    "@types/commander": "^2.12.2",
    "@types/diff": "^5.0.2",
    "@types/jest": "^27.5.0",
    "@types/line-column": "^1.0.0",
    "@types/node": "^17.0.23",
    "@types/readline-sync": "^1.4.4",
    "@types/string-format": "^2.0.0",
    "ariadne-ts": "^0.1.5-alpha.10",
    "chai": "^4.3.6",
    "cli-highlight": "^2.1.11",
    "colors": "^1.4.0",
    "commander": "^9.1.0",
    "diff": "^5.1.0",
    "fork-ts-checker-notifier-webpack-plugin": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "highlight.js": "^11.5.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.0.3",
    "line-column": "^1.0.2",
    "nodemon": "^2.0.16",
    "platform-folders": "^0.6.0",
    "pretty-repl": "^3.1.1",
    "process": "^0.11.10",
    "readline-sync": "^1.4.10",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "string-format": "^2.0.0",
    "ts-jest": "^28.0.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3",
    "util": "^0.12.4",
    "vite-tsconfig-paths": "^3.5.0",
